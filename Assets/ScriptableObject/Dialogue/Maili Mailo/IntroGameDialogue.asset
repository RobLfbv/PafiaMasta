    emotion: 3
    choices:
    - textChoice: 
      idNext: 0
      method: 0
    idGoto: 14
    isChoiceDialogue: 0
  - idDialogue: 14
    charTalking: 7
    text: "Allez les interroger et je vous conseille de commencer par Yette, il se
